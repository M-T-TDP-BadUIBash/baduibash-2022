{
  "name": "currency-design-system",
  "version": "4.2.0",
  "description": "Official SCSS library for M&T's Currency design system.",
  "main": "index.html",
  "devDependencies": {
    "node-sass": "^4.11.0",
    "nodemon": "^1.18.9",
    "sass-recursive-map-merge": "^1.0.1"
  },
  "scripts": {
    "build-basic": "node-sass --include-path scss scss/mtb-currency-design.scss   css/mtb-currency-design.css",
    "build-defaults": "node-sass --include-path scss scss/mtb-currency-design--defaults-set.scss   css/mtb-currency-design--defaults-set.css",
    "build-components": "node-sass --include-path scss scss/mtb-currency-design--components.scss   css/mtb-currency-design--components.css",
    "build-layouts": "node-sass --include-path scss scss/mtb-currency-design--layouts.scss   css/mtb-currency-design--layouts.css",
    "build-patterns": "node-sass --include-path scss scss/mtb-currency-design--patterns.scss   css/mtb-currency-design--patterns.css",
    "build-utilities": "node-sass --include-path scss scss/mtb-currency-design--utilities.scss   css/mtb-currency-design--utilities.css",
    "build-all": "npm run build-basic & npm run build-defaults & npm run build-components & npm run build-layouts & npm run build-patterns & npm run build-utilities",
    "build-wtc-basic": "node-sass --include-path scss scss/wtc-currency-design.scss   css/wtc-currency-design.css",
    "build-wtc-defaults": "node-sass --include-path scss scss/wtc-currency-design--defaults-set.scss   css/wtc-currency-design--defaults-set.css",
    "build-wtc-components": "node-sass --include-path scss scss/wtc-currency-design--components.scss   css/wtc-currency-design--components.css",
    "build-wtc-layouts": "node-sass --include-path scss scss/wtc-currency-design--layouts.scss   css/wtc-currency-design--layouts.css",
    "build-wtc-patterns": "node-sass --include-path scss scss/wtc-currency-design--patterns.scss   css/wtc-currency-design--patterns.css",
    "build-wtc-utilities": "node-sass --include-path scss scss/wtc-currency-design--utilities.scss   css/wtc-currency-design--utilities.css",
    "build-wtc-all": "npm run build-wtc-basic & npm run build-wtc-defaults & npm run build-wtc-components & npm run build-wtc-layouts & npm run build-wtc-patterns & npm run build-wtc-utilities",
    "build-full": "npm run build-wtc-basic & npm run build-wtc-defaults & npm run build-wtc-components & npm run build-wtc-layouts & npm run build-wtc-patterns & npm run build-wtc-utilities & npm run build-basic & npm run build-defaults & npm run build-components & npm run build-layouts & npm run build-patterns & npm run build-utilities",
    "watch-css": "nodemon -e scss -x \"npm run build-css\""
  },
  "repository": {
    "type": "git",
    "url": "git+adomtb@dev.azure.com/adomtb/CurrencyDesignSystem/_git/CurrencySCSS"
  },
  "author": "M&T Design System Team",
  "license": "ISC",
  "homepage": "https://dev.azure.com/adomtb/CurrencyDesignSystem/_git/CurrencySCSS",
  "publishConfig": {
    "registry": "https://bar.prod.mtb.com/artifactory/api/npm/mtb-npm-ga"
  }
}
